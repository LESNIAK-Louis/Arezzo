      <Menu mnemonicParsing="false" text="Outils">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#renommerMelodie" text="Renommer" />
            <MenuItem mnemonicParsing="false" onAction="#transposerMelodie" text="Transposer" />
         </items>
      </Menu>
